{
  "name": "react-boilerplate",
  "version": "0.1.0",
  "description": "Boilerplate to develop Webapps based on React",
  "scripts": {
    "start": "BABEL_ENV=dev node server.js",
    "pretest": "npm run lint && npm run typecheck",
    "lint": "eslint src test",
    "typecheck": "flow",
    "test": "BABEL_ENV=test mocha --compilers js:babel-register --require test/setup.js test/**/*.spec.js",
    "build": "BABEL_ENV=dev webpack --progress -p --env.prod"
  },
  "author": "Ariel Schiavoni",
  "devDependencies": {
    "babel-core": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-class-properties": "^6.10.2",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2015-webpack": "^6.4.1",
    "babel-preset-react": "^6.11.1",
    "babel-register": "^6.9.0",
    "chai": "^3.5.0",
    "clean-webpack-plugin": "^0.1.10",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.23.1",
    "enzyme": "^2.4.1",
    "eslint": "^3.1.1",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-react": "^5.2.2",
    "extract-text-webpack-plugin": "^2.0.0-beta.3",
    "file-loader": "^0.9.0",
    "flow-bin": "^0.30.0",
    "html-webpack-plugin": "^2.22.0",
    "jsdom": "^9.4.1",
    "json-loader": "^0.5.4",
    "mocha": "^2.5.3",
    "postcss-cssnext": "^2.7.0",
    "postcss-loader": "^0.9.1",
    "react-addons-test-utils": "^15.2.1",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "redux-logger": "^2.6.1",
    "sinon": "^1.17.4",
    "webpack": "^2.1.0-beta.21",
    "webpack-dashboard": "^0.1.1",
    "webpack-dev-server": "^2.1.0-beta.0"
  },
  "dependencies": {
    "aphrodite": "^0.5.0",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2"
  }
}
