{
  "env": {
    "dev": {
       "presets": ["es2015-webpack", "react"],
       "plugins": [
         "transform-object-rest-spread",
         "transform-class-properties",
         "transform-flow-strip-types",
         "transform-runtime"
       ]
     },
    "test": {
      "presets": ["es2015", "react"],
      "plugins": [
        "transform-object-rest-spread",
        "transform-class-properties",
        "transform-flow-strip-types",
        "transform-runtime"
      ]
    }
  }
}
