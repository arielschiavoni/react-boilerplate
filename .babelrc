{
  "env": {
    "development": {
       "presets": [
         ["es2015", { "modules": false }],
         "react"
       ],
       "plugins": [
         "transform-object-rest-spread",
         "transform-class-properties",
         "transform-flow-strip-types"
       ]
     },
    "test": {
      "presets": ["es2015", "react"],
      "plugins": [
        "transform-object-rest-spread",
        "transform-class-properties",
        "transform-flow-strip-types"
      ]
    }
  }
}
